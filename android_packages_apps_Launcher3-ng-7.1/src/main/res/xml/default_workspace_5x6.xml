<?xml version="1.0" encoding="utf-8"?>

<favorites xmlns:launcher="http://schemas.android.com/apk/res-auto/com.enrico.launcher3">

    <!-- Hotseat -->
    <include launcher:workspace="@xml/dw_tablet_hotseat" />

    <!-- Bottom row -->
    <favorite
        launcher:screen="0"
        launcher:uri="#Intent;action=android.intent.action.MAIN;category=android.intent.category.APP_CONTACTS;end"
        launcher:x="0"
        launcher:y="-1" />

    <resolve
        launcher:screen="0"
        launcher:x="-1"
        launcher:y="-1">
        <favorite launcher:uri="#Intent;action=android.intent.action.MAIN;category=android.intent.category.APP_MARKET;end" />
        <favorite launcher:uri="market://details?id=com.android.launcher" />
    </resolve>

</favorites>
